        async function CargarTodo() {
           await Cargar_Valorizacion();
           await Cargar_ObserTotal();
           await Cargar_ObserParcial();
           await Cargar_Diagnosticos();
           await Cargar_Medicamentos();
           await Cargar_Procedimientos();
           await Cargar_Insumos();
        }


     async function Cargar_Valorizacion() {
	    CargarWaitMe();
        let id = $('#Ate_Idnumreg').val();
        try {

            const response = await $.get("@Url.Content("~/VisorFUA/ValorizacionFUAV")", { id: id});
            $('#tabla_Valorizacion').html(response);
            TerminarWaitMe();

        } catch {
            console.log('Error Obs Total');
            TerminarWaitMe();
        }

    }

	async function Cargar_ObserTotal() {
	    CargarWaitMe();
        let id = $('#Ate_Idnumreg').val();
        let v_periodo = $('#v_periodo').val();
        let v_mes = $('#v_mes').val();
        try {

            const response = await $.get("@Url.Content("~/VisorFUA/ObserTotalFUAV")", { id: id, v_periodo: v_periodo, v_mes: v_mes });
            $('#tabla_ObsTotal').html(response);
            if ($("#TotalObs_cant").length == 1) {
                $('#tblObserTotal').DataTable({
                    destroy: true,
                    "ordering": false,
                    responsive: true,
                    "lengthChange": false,
                    "searching": false,
                    "language": languaje_spanol
                });
                TerminarWaitMe();
            } else {
                TerminarWaitMe();
            }

        } catch {
            console.log('Error Obs Total');
            TerminarWaitMe();
        }

    }

	async function Cargar_ObserParcial() {
			CargarWaitMe();
            let id = $('#Ate_Idnumreg').val();
            let v_periodo = $('#v_periodo').val();
        let v_mes = $('#v_mes').val();

        try {

            const response = await $.get("@Url.Content("~/VisorFUA/ObserParcialFUAV")", { id: id, v_periodo: v_periodo, v_mes: v_mes });
            $('#tabla_ObsParcial').html(response);
            if ($("#ParcialOBS_cant").length == 1) {
                $('#tblObserParcial').DataTable({
                    destroy: true,
                    "ordering": false,
