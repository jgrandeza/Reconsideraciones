@model IEnumerable<App.ViewModels.SELSiasis.SELSiasis.getAtencionAPOObs>

<table class="table align-middle border rounded table-row-dashed fs-6 g-5 display responsive" id="tblProcedimientos" style="font-size: 10px !important">
    <thead class="text-center" style="background-color: #0272A3; border-width: 2px; color: white; vertical-align: middle; ">
        <tr>
            <th style="text-align:center"><span>N</span></th>
            <th style="text-align:center">CODIGO</th>
            <th style="text-align:center">DESCRIPCIÓN</th>
            <th style="text-align:center">DX</th>
            <th style="text-align:center">IND</th>
            <th style="text-align:center">EJE</th>
            <th style="text-align:center">CANT APROB. UDR</th>
            <th style="text-align:center">COSTO(S/.)</th>
            <th style="text-align:center">COSTO TOTAL(S/.)</th>
            <th style="text-align:center">TIPO PRECIO</th>
            <th style="text-align:center">OBSERVACIÓN</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (Model.Count() > 0)
            {
                var contador = 0;
                foreach (var item in Model)
                {
                <tr id="ApoObs_cant">
                    <td class="text-center">@(contador + 1)</td>
                    <td class="text-center">@item.AAPO_CODAPO</td>
                    <td class="text-center">@item.APO_NOMBRE</td>
                    <td class="text-center">@item.AAPO_INRODIA</td>
                    <td class="text-center">@item.AAPO_ICANTPROCED</td>
                    <td class="text-center">@item.AAPO_ICANTEJECUTADA</td>
                    <td class="text-center">@item.AAPO_ICANTIDADAPROBADAODSIS</td>
                    <td class="text-center">@item.AAPO_NCOSTOAPLICADO</td>
                    <td class="text-center">@item.AAPO_NCOSTOAPLICADO</td>
                    <td class="text-center">@item.pra_tipo_precio_des</td>
                    <td class="text-center">@item.AAPO_OBSERVACIONES</td>


                </tr>
                    contador++;
                }
            }
            else
            {
                <tr>
                    <td colspan="11"><span class="text-danger">No hay registros para mostrar</span></td>
                </tr>
            }
        }
    </tbody>
</table>
<script>

</script>