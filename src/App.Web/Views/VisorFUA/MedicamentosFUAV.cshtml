@model IEnumerable<App.ViewModels.SELSiasis.SELSiasis.getAtencionMEDObs>

<table class="table align-middle border rounded table-row-dashed fs-6 g-5 display responsive" id="tblMedicamentos" style="font-size: 10px !important">
    <thead class="text-center" style="background-color: #0272A3; border-width: 2px; color: white; vertical-align: middle; ">
        <tr>
            <th style="text-align:center"><span>N</span></th>
            <th style="text-align:center">CODIGO</th>
            <th style="text-align:center">DESCRIPCIÓN</th>
            <th style="text-align:center">FF</th>
            <th style="text-align:center">CONCENTR</th>
            <th style="text-align:center">DX</th>
            <th style="text-align:center">PRES</th>
            <th style="text-align:center">ENTR</th>
            <th style="text-align:center">ENTR UDR</th>
            <th style="text-align:center">COSTO(S/.)</th>
            <th style="text-align:center">COSTO TOTAL(S/.)</th>
            <th style="text-align:center">TIPO PRECIO</th>
            <th style="text-align:center">OBSERVACIÓN</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (Model.Count() > 0)
            {
                var contador = 0;
                foreach (var item in Model)
                {
                <tr id="MedObs_cant">
                    <td class="text-center">@(contador + 1)</td>
                    <td class="text-center">@item.AMED_CODMED</td>
                    <td class="text-center">@item.MED_NOMBRE</td>
                    <td class="text-center">@item.MED_PRESEN</td>
                    <td class="text-center">@item.MED_CONCEN</td>
                    <td class="text-center">@item.AMED_INRODIA</td>
                    <td class="text-center">@item.AMED_ICANTPRESCRITA</td>
                    <td class="text-center">@item.AMED_ICANTENTREGADA</td>
                    <td class="text-center">@item.AMED_ICANTAPROBADAODSIS</td>
                    <td class="text-center">@item.MED_COSTO</td>
                    <td class="text-center">@item.AMED_IPRECIOTOTAL</td>
                    <td class="text-center">@item.PRA_TIPO_PRECIO_DES</td>
                    <td class="text-center">@item.AMED_OBSERVACIONES</td>

                </tr>
                    contador++;
                }
            }
            else
            {
                <tr>
                    <td colspan="12"><span class="text-danger">No hay registros para mostrar</span></td>
                </tr>
            }
        }
    </tbody>
</table>
<script>

</script>