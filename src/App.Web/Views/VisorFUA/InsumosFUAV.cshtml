@model IEnumerable<App.ViewModels.SELSiasis.SELSiasis.getAtencionINSObs>

<table class="table align-middle border rounded table-row-dashed fs-6 g-5 display responsive" id="tblInsumos" style="font-size: 10px !important">
    <thead class="text-center" style="background-color: #0272A3; border-width: 2px; color: white; vertical-align: middle; ">
        <tr>
            <th style="text-align:center"><span>N</span></th>
            <th style="text-align:center">CODIGO</th>
            <th style="text-align:center">DESCRIPCIÓN</th>
            <th style="text-align:center">DX</th>
            <th style="text-align:center">PRES</th>
            <th style="text-align:center">ENTR</th>
            <th style="text-align:center">CANT APROB UDR</th>
            <th style="text-align:center">COSTO(S/.)</th>
            <th style="text-align:center">COSTO TOTAL(S/.)</th>
            <th style="text-align:center">TIPO PRECIO</th>
            <th style="text-align:center">OBSERVACIÓN</th>
        </tr>
    </thead>
    <tbody>
        @{
            if (Model.Count() > 0)
            {
                var contador = 0;
                foreach (var item in Model)
                {
                <tr id="InsObs_cant">
                    <td class="text-center">@(contador + 1)</td>
                    <td class="text-center">@item.AINS_CODINS</td>
                    <td class="text-center">@item.INS_NOMBRE</td>
                    <td class="text-center">@item.AINS_INRODIA</td>
                    <td class="text-center">@item.AINS_ICANTPRESCRITA</td>
                    <td class="text-center">@item.AINS_ICANTENTREGADA</td>
                    <td class="text-center">@item.AINS_ICANTIDADAPROBADAODSIS</td>
                    <td class="text-center">@item.INS_COSTO</td>
                    <td class="text-center">@item.AINS_IPRECIOTOTAL</td>
                    <td class="text-center">@item.PRA_TIPO_PRECIO_DES</td>
                    <td class="text-center">@item.AINS_OBSERVACIONES</td>

                </tr>
                    contador++;
                }
            }
            else
            {
                <tr>
                    <td colspan="8"><span class="text-danger">No hay registros para mostrar</span></td>
                </tr>
            }
        }
    </tbody>
</table>
<script>

</script>