@model IEnumerable<App.ViewModels.SELReconsideraciones.getAtencionesINSxID>

<table class="table align-middle border rounded table-row-dashed fs-6 g-5 display responsive" id="tblInsumos" style="font-size: 10px !important">
    <thead class="text-center" style="background-color: #0272A3; border-width: 2px; color: white; vertical-align: middle; ">
        <tr>
            <th style="text-align:center">Nº</th>
            <th style="text-align:center">CODIGO</th>
            <th style="text-align:center">DESCRIPCIÓN</th>
            <th style="text-align:center">PO(S/.)</th>
            <th style="text-align:center">PR</th>
            <th style="text-align:center">Carac</th>
            <th style="text-align:center">PRES</th>
            <th style="text-align:center">ENTR</th>
            <th style="text-align:center">DX</th>
            <th style="text-align:center">OBSERVACIÓN</th>
            <th style="text-align:center">PREPOS(S/.)</th>
            @if (ViewBag.RecEstado == 6 && ViewBag.Permite == 1)
                {
                <th style="text-align:center"><span>ACCIÓN</span></th>

            }
        </tr>
    </thead>
    <tbody>
        @{

            if (Model.Count() > 0)
            {
                var i = 1;
                foreach (var item in Model)
                {
                <tr id="Insumos_cant">
                    <td style="text-align: center"> @i</td>
                    <td style="text-align: justify">@item.AINS_CODINS</td>
                    <td style="text-align: justify">@item.INS_NOMBRE</td>
                    <td style="text-align: center"><b>@( string.Format("{0:C2}", item.INS_COSTO))</b></td>
                    <td style="text-align: center">@item.INS_PRESEN</td>
                    <td style="text-align: center">@item.AINS_CCARACT</td>
                    <td style="text-align: center">@item.AINS_ICANTPRESCRITA</td>
                    <td style="text-align: center">@item.AINS_ICANTENTREGADA</td>
                    <td style="text-align: justify">@item.C10_NOMBRE</td>
                    <td style="text-align: justify">@item.ROBS_DETALLE</td>
                    <td style="text-align: justify"></td>
                    @if (ViewBag.RecEstado == 6 && ViewBag.Permite == 1)
                        {
                        <td style="text-align: center">
                            <a onclick="Conf_Eliminar(event,4,@item.AINS_IDNUMREG);" href="javascript:void(0);"><i style="font-size: 20px; color: #c64821;" title="Eliminar Insumos" class="fas fa-trash" aria-hidden="true"></i></a>
                        </td>
                    }

                </tr>
                    i = i + 1;
                }
            }
            else
            {
                <tr>
                    <td colspan="11" style="text-align: center"><h5 class="text-danger">Aún no hay registros a mostrar</h5></td>
                </tr>
            }

        }
    </tbody>
</table>
<script></script>