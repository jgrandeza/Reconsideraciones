                                    <option selected disabled value="">Seleccionar</option>
                                    @{ if (ViewBag.ListEspecialidad != null)
                                        {
                                            foreach (var item in ViewBag.ListEspecialidad)
                                            {
                                                                <option value="@item.ESP_IDESPECIALIDAD">@item.ESP_CNOMBRE</option>
