<script>
    $(".date").datepicker({
        format: "dd/mm/yyyy",
        autoclose: true,
        todayHighlight: true,
        language: "es"
    });
</script>

<script>
    setTimeout(() => { CargarDatosIG(); }, 600);

    function Formato_Fecha(fecha) {
        return fecha.substring(8, 10) + "/" + fecha.substring(5, 7) + "/" + fecha.substring(0, 4);
    }

    function CargarDatosIG() {
        CargarWaitMe();
        let id = $("#Ate_Idnumreg").val();
        var formData = new FormData();
        formData.append("id", id);

        $.ajax({
            url: '@Url.Action("AtencionxID", "FUA")',
            type: "POST",
            enctype: 'multipart/form-data',
            data: formData,
            processData: false,
            contentType: false,
            cache: false,
            timeout: 600000,

            error: function (e) {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
                //TerminarWaitMe();
                console.log("ERROR : ", e.responseText);
            }

        }).done(function (response) {
            var data = response.result;
            console.log(data);
            if (response.isSuccess == true) {
                //$("#lblProyecto2").html(data.proNombreProyectoInversion);

                $("#txtFUA").val(data.atE_DISA +"-"+ data.atE_LOTE +"-"+ data.atE_NUMREGATE );
                $("#txtCostoObservado").val(data.atE_VALOR_BRUTO- data.atE_VALOR_NETO);

                 $("#txtPerProduc").val(data.atE_PERIODO + "-"+ data.atE_MES);

      
                $("#txtIPRESS").val(data.atE_IDEESS);

                CONS_IPRESS();


                //3. DATOS ASEGURAD0

                $("#cbxComponente").val(data.atE_IDCOMPONENTE);
                $('#cbxComponente').trigger('change');
                $("#cbxTipoFormato").val(data.atE_TIPOFORMATOAFIINS);
                $('#cbxTipoFormato').trigger('change');
                $("#txtCodAfiliado").val(data.atE_DISAAFIINS);
                $("#txtCodAfiliado1").val(data.atE_TIPOFORMATOAFIINS);
                $("#txtCodAfiliado2").val(data.atE_NUMREGAFIINS);
                $("#cbxTipoDocumAfi").val(data.atE_TIPODOCUMENTO);
                $('#cbxTipoDocumAfi').trigger('change');
                $("#txtDocIdentAfi").val(data.atE_DNI);
                $("#txtApePaterAfi").val(data.atE_APPAT);
                $("#txtApeMaterAfi").val(data.atE_APMAT);
                $("#txtNombreAfi").val(data.atE_PNOM);
                $("#txtOtroNombreAfi").val(data.atE_SNOM);
                $('#txtFechNaciAfi').datepicker('setDate', Formato_Fecha(data.atE_FECNAC));

                $("#txtEdadAfi").val(data.atE_EDAD);
                $("#cbxGeneroAfi").val(data.atE_IDSEXO);
                $('#cbxGeneroAfi').trigger('change');
                $("#cbxTipoAtencion").val(data.atE_IDMODALIDAD);
                $('#cbxTipoAtencion').trigger('change');
                //falta prestacion

                $("#cbxGestante").val(data.atE_ESGESTANTE);
                $('#cbxGestante').trigger('change');
                if (data.atE_ESGESTANTE != 0) {
                    $('#txtFechPartoAfi').datepicker('setDate', Formato_Fecha(data.atE_FECPARTO));

                }
                $("#txtHCAfi").val(data.atE_HISTORIACLINICA);
                $("#txtNroAutoriz").val(data.atE_NUMAUTORIZACION);
                $("#txtMontoAfi").val(data.atE_MONTO);

                //DATOS DE LA ATENCIÓN
                $('#txtFechAte').datepicker('setDate', Formato_Fecha(data.atE_FECATENCION));

                $("#cbxLugarAte").val(data.atE_IDLUGAR);
                $('#cbxLugarAte').trigger('change');
                $("#cbxPersonalAte").val(data.atE_IDORIGENPERSONAL);
                $('#cbxPersonalAte').trigger('change');
                $("#txtCodPrestacion").val(data.atE_IDSERVICIO);
                CargarServicio(data.atE_IDSERVICIO);
                $("#txtEESSRef").val(data.atE_IDEESSREFIRIO);

                EESSRefe();

                $("#txtNumRef").val(data.atE_NROHOJAREFERENCIA);
                $('#txtFechIngresoAte').datepicker('setDate', Formato_Fecha(data.atE_FECINGHOSP));

                $("#cbxDestinoRefAte").val(data.atE_CODDESTINO);
                $('#cbxDestinoRefAte').trigger('change');
                $('#txtFechAltaAte').datepicker('setDate', Formato_Fecha(data.atE_FECALTAHOSP));


                $("#txtEESSContraRef").val(data.atE_IDEESSCONTRAREFIERE);
                EESSContrRefe();

                $("#txtNumContraRef").val(data.atE_NUMHOJACONTRAREFIERE);

                //RESPONSABLE DE LA ATENCION

                $("#cbxTipoDocu").val(data.atE_TIPODOCPERSONALSALUD);
                $('#cbxTipoDocu').trigger('change');
                $("#txtDocuIdent").val(data.atE_DNIPERSONALSALUD);
                CargarPersonalSalud(data.atE_DNIPERSONALSALUD);
                $("#txtColegiatura").val(data.atE_COLEGIATURAPERSONALSALUD);
                $("#cbxProfesional").val(data.atE_IDTIPOPERSONALSALUD);
                $('#cbxProfesional').trigger('change');

                //OBSERVACION
                $("#txtobservacion").val(data.atE_OBSERVACION);  
                


            } else {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
            }

        }).fail(function () {

        }).always(function () {
            TerminarWaitMe();
        });
    }

    async function CONS_IPRESS() {
        let eess = $("#txtIPRESS").val();
        try {
            const { result } = await CargarEESS(eess)
            console.log(result);
            $("#txtEESS").val(result.nombre);
            $("#txtCategoria").val(result.abrvcat);
            $("#txtUEFUA").val(result.desejec);
        } catch(err) {
            console.log(err);
        }
    }
    async function EESSRefe() {
        let eess = $("#txtEESSRef").val();
        try {
            const { result } = await CargarEESS(eess)
            console.log(result);
            $("#txtDescEESSRef").val(result.nombre);
        } catch (err) {
            console.log(err);
        }
    }
    async function EESSContrRefe() {
        let eess = $("#txtEESSContraRef").val();
        try {
            const { result } = await CargarEESS(eess)
            console.log(result);
            $("#txtDescEESSContraRef").val(result.nombre);
        } catch (err) {
            console.log(err);
        }
    }

    async function CargarEESS(eess) {
        if (eess == "" || eess === null) {
            return false;
        }

        let id = eess;
        var formData = new FormData();
        formData.append("id", id);

        return $.ajax({
            url: '@Url.Action("EESSxID", "FUA")',
            type: "POST",
            enctype: 'multipart/form-data',
            data: formData,
            processData: false,
            contentType: false,
            cache: false,
            timeout: 600000,
            async: true,

            error: function (e) {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
                //TerminarWaitMe();
                console.log("ERROR : ", e.responseText);
            }

        });
    }

    function CargarServ() {
        let id=$("#txtCodPrestacion").val();
        CargarServicio(id);
    }


    function CargarServicio(srv) {
        if (srv == "" || srv === null || srv.length != 3) {
            NotificacionToastr('error', `Ingrese Codigo de Servicio correcto.`, 'ALERTA');
            return false;
        }

        let id = srv;
        var formData = new FormData();
        formData.append("id", id);

        $.ajax({
            url: '@Url.Action("ServicioxID", "FUA")',
            type: "POST",
            enctype: 'multipart/form-data',
            data: formData,
            processData: false,
            contentType: false,
            cache: false,
            timeout: 600000,

            error: function (e) {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
                //TerminarWaitMe();
                console.log("ERROR : ", e.responseText);
            }

        }).done(function (response) {
            var data = response.result;
            if (response.isSuccess == true) {
                $('#txtServicio').val(data.seR_NOMBRE);
            } else {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
            }

        }).fail(function () {

        }).always(function () {

        });
    }

    function CargaPersSalud(){
        let pers=$("#txtDocuIdent").val();
        CargarPersonalSalud(pers);
    }

    function CargarPersonalSalud(srv) {
        if (srv == "" || srv === null) {
            NotificacionToastr('error', `Ingrese el Documento de Identidad correcto.`, 'ALERTA');
            return false;
        }

        let id = srv;
        var formData = new FormData();
        formData.append("id", id);

        $.ajax({
            url: '@Url.Action("PersonalSaludxID", "FUA")',
            type: "POST",
            enctype: 'multipart/form-data',
            data: formData,
            processData: false,
            contentType: false,
            cache: false,
            timeout: 600000,

            error: function (e) {
                NotificacionToastr('error', `Error al conectarse?`, 'ALERTA');
                //TerminarWaitMe();
                console.log("ERROR : ", e.responseText);
            }

        }).done(function (response) {
            var data = response.result;
            if (response.isSuccess == true) {
                console.log(data);
                $('#txtRespAte').val(data.nombrePersonal);
                $('#cbxProfesional').val(parseInt(data.idTipoPersonal));
                $('#txtColegiatura').val(data.colegiatura);
                $('#cbxEspecialidad').val(data.idEspecialidad);
                $('#cbxEspecialidad').trigger('change');
                               
                //idEspecialidad
                //cespecialidad
                
            } else {
                NotificacionToastr('error', `El Documento Ingresado no existe?`, 'ALERTA');
                $('#txtRespAte').val('');
            }

        }).fail(function () {

        }).always(function () {

        });
    }

    function actualizar_datos_FUA(){
        Swal.fire({
          title: '¿Estas Seguro?',
          text: "¡No podrás revertir esto!",
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
          cancelButtonText: 'Cancelar',
          confirmButtonText: 'Si, Actualizar'
        }).then((result) => {
          if (result.isConfirmed) {
            _actualizar_datos_FUA();
          }
        })
    }

    function _actualizar_datos_FUA(){
        const FUA = {};

        FUA.ATE_IDNUMREG = $("#Ate_Idnumreg").val();
        FUA.ATE_IDCOMPONENTE = $("#cbxComponente").val();
        FUA.ATE_DISAAFIINS = $("#txtCodAfiliado").val();
        FUA.ATE_TIPOFORMATOAFIINS = $("#txtCodAfiliado1").val();
        FUA.ATE_NUMREGAFIINS = $("#txtCodAfiliado2").val();
        FUA.ATE_TIPODOCUMENTO = $("#cbxTipoDocumAfi").val();
        FUA.ATE_DNI = $("#txtDocIdentAfi").val();
        FUA.ATE_APPAT = $("#txtApePaterAfi").val();
        FUA.ATE_APMAT = $("#txtApeMaterAfi").val();
        FUA.ATE_PNOM = $("#txtNombreAfi").val();
        FUA.ATE_SNOM = $("#txtOtroNombreAfi").val();
        FUA.ATE_FECNAC = $("#txtFechNaciAfi").val();
        FUA.ATE_EDAD = $("#txtEdadAfi").val();
        FUA.ATE_IDSEXO = $("#cbxGeneroAfi").val();
        FUA.ATE_IDMODALIDAD = $("#cbxTipoAtencion").val();
        FUA.ATE_ESGESTANTE = $("#cbxGestante").val();
        FUA.ATE_FECPARTO = $("#txtFechPartoAfi").val();
        FUA.ATE_HISTORIACLINICA = $("#txtHCAfi").val();
        FUA.ATE_NUMAUTORIZACION = $("#txtNroAutoriz").val();
        FUA.ATE_MONTO = $("#txtMontoAfi").val();
        FUA.ATE_FECATENCION = $("#txtFechAte").val();
        FUA.ATE_IDLUGAR = $("#cbxLugarAte").val();
        FUA.ATE_IDORIGENPERSONAL = $("#cbxPersonalAte").val();
        FUA.ATE_IDSERVICIO = $("#txtCodPrestacion").val(); 
        FUA.ATE_IDEESSREFIRIO = $("#txtEESSRef").val();
        FUA.ATE_NROHOJAREFERENCIA = $("#txtNumRef").val();
        FUA.ATE_FECINGHOSP = $("#txtFechIngresoAte").val();
        FUA.ATE_CODDESTINO = $("#cbxDestinoRefAte").val();
        FUA.ATE_FECALTAHOSP = $("#txtFechAltaAte").val();
        FUA.ATE_IDEESSCONTRAREFIERE = $("#txtEESSContraRef").val();
        FUA.ATE_NUMHOJACONTRAREFIERE = $("#txtNumContraRef").val();;
        FUA.ATE_TIPODOCPERSONALSALUD = $("#cbxTipoDocu").val();;
        FUA.ATE_DNIPERSONALSALUD = $("#txtDocuIdent").val();;
        FUA.ATE_COLEGIATURAPERSONALSALUD = $("#txtColegiatura").val();
        FUA.ATE_IDTIPOPERSONALSALUD = $("#cbxProfesional").val();
        FUA.ATE_OBSERVACION = $("#txtobservacion").val();

        $.post('@Url.Action("ActualizarDatosFUA","FUA")', FUA)
          .done(function(response) {
            if(response.isSuccess == true){
                NotificacionToastr('success', `${response.message}`, 'Mensaje de confirmación');
            }
            else{
                NotificacionToastr('error', `${response.message}`, 'Error..!');
            }
          });
    }

</script>